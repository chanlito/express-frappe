{
  "name": "express-frappe",
  "version": "1.0.0",
  "description": "Express Frappe ðŸ¥¤ - build express application another way.",
  "keywords": [
    "express",
    "expressjs",
    "typescript"
  ],
  "author": "Chanlito Hem <chanlitohem@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/chanlito/express-frappe"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "build": "npx tsc",
    "clean": "rm -rf dist",
    "coverage": "nyc mocha && cat ./coverage/lcov.info | coveralls",
    "prepare": "npm run clean && npm run build",
    "test": "mocha"
  },
  "peerDependencies": {
    "express": "^4.16.3"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "express-routers": "^1.0.1",
    "morgan": "^1.9.0",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.2",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "all": true
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
